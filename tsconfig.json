{
    "compilerOptions": {
      "target": "es5",
      "module": "es6",
      "moduleResolution": "node",
      "strict": true,
      "jsx": "react-jsx",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "sourceMap": true
    },
    "include": [
      "src/**/*"
    ]
  }
  